package AtividadePS7;

/**
 *
 * @author jhonathanmilk
 */
public class Sindicato {
    private double taxaMensal = .1; //taxa mensal 10%

    public double getTaxaMensal() {
        return taxaMensal;
    }

    public void setTaxaMensal(double taxaMensal) {
        this.taxaMensal = taxaMensal;
    }
    
    public double taxaSindicato(double salario) {
        return salario*taxaMensal;
    }
}
